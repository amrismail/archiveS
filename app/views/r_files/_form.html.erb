  </div>

  <div class="field">
    <%= f.label :event %>
    <%= f.text_field :event %>
  </div>

  <div class="field">
    <%= f.label :place %>
    <%= f.text_field :place %>
  </div>

  <div class="field">
    <%= f.label :access %>
    <%= f.number_field :access %>
  </div>

  <div class="field">
    <%= f.label :URL %>
    <%= f.text_field :URL %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
