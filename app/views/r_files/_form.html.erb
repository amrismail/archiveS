<%= form_for(r_file) do |f| %>
  <% if r_file.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(r_file.errors.count, "error") %> prohibited this r_file from being saved:</h2>

      <ul>
      <% r_file.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
